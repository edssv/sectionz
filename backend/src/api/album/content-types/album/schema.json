{
  "kind": "collectionType",
  "collectionName": "albums",
  "info": {
    "singularName": "album",
    "pluralName": "albums",
    "displayName": "Album",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "genre": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "tracks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::track.track",
      "mappedBy": "album"
    },
    "albumType": {
      "type": "enumeration",
      "enum": [
        "album",
        "single"
      ],
      "required": true
    },
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "albums"
    },
    "releaseDate": {
      "type": "datetime",
      "required": true
    },
    "durationSec": {
      "type": "float",
      "required": true,
      "default": 0
    }
  }
}
