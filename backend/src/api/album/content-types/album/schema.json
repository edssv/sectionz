{
  "kind": "collectionType",
  "collectionName": "albums",
  "info": {
    "singularName": "album",
    "pluralName": "albums",
    "displayName": "Album",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "string"
    },
    "genre": {
      "type": "string"
    },
    "artist": {
      "type": "string",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "tracks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::track.track",
      "mappedBy": "album"
    }
  }
