{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "singularName": "track",
    "pluralName": "tracks",
    "displayName": "Track",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "genre": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "audio": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "audios"
      ]
    },
    "album": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::album.album",
      "inversedBy": "tracks"
    },
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "tracks"
    },
    "plays_number": {
      "type": "biginteger",
      "default": "0",
      "min": "0"
    },
    "duration_ms": {
      "type": "float"
    }
  }
}
