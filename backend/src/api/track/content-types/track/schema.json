{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "singularName": "track",
    "pluralName": "tracks",
    "displayName": "Track"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "artist": {
      "type": "string",
      "required": true
    },
    "genre": {
      "type": "string"
    },
    "year": {
      "type": "string"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "audio": {
      "allowedTypes": [
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "album": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::album.album",
      "inversedBy": "tracks"
    }
  }
}
